generator client {
  provider = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model test {
  id    BigInt  @id(map: "test_pk") @default(sequence())
  hello String?
}

model posts {
  id   BigInt  @id(map: "posts_pk") @default(sequence())
  slug String?
}
