generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model posts {
  id   BigInt  @id(map: "posts_pk") @default(sequence())
  slug String?
}

model links {
  id   BigInt  @id @default(autoincrement())
  page String
  icon String?
  text String?
  url  String
}
