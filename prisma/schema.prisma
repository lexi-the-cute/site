generator client {
  provider   = "prisma-client-js"
  engineType = "binary"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model links {
  id   BigInt  @id @default(autoincrement())
  page String
  icon String?
  text String?
  url  String
}

model authors {
  id          BigInt  @id(map: "authors_pk") @default(autoincrement())
  author      String  @unique(map: "authors_un")
  pronouns_id String?
}
